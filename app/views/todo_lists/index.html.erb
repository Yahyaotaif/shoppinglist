
<% @todo_lists.each do |todo_list|%>
    <div class="index_row clearfix">
      <div class="todo_list_title">
        <p><%= link_to todo_list.title, todo_list %></p>
      </div>
      <div class="trash">
        <%= link_to todo_list, method: :delete, data: { confirm: 'Are you sure?' } do %>
        <p> <i class="fa fa-trash"></i> </p>
            <% end %>
      </div>
    </div>


<%end%>
<div class="links">
  <%= link_to "New Shopping List", new_todo_list_path %>
</div>



